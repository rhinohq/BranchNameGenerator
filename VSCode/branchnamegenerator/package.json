{
	"name": "branchnamegenerator",
	"displayName": "BranchNameGenerator",
	"description": "Extension for generating branch names.",
	"version": "0.0.3",
	"publisher": "RKendrick",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],

	"repository": {
		"type": "git",
		"url": "https://github.com/rhinohq/BranchNameGenerator.git"
	},
	"activationEvents": [
		"onCommand:extension.branchNameGenerator"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.branchNameGenerator",
				"title": "Branch Name Generator"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
